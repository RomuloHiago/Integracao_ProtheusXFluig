      <workflowProcessEventPK>
        <companyId>1</companyId>
        <eventId>beforeTaskSave</eventId>
        <processId>Novo_Ponto_Comercial</processId>
        <version>1</version>
      </workflowProcessEventPK>
      <eventDescription>/*	Estou usando o evento &apos;beforeTaskSave&apos;(antes da solicitação ser salva) antes de ela ser salvo, &#xd;
 	estou validando e impedir que o user crie uma solicição sem inserir a proposta comercial (PDF anexado).&#xd;
 	&#xd;
*/&#xd;
&#xd;
function beforeTaskSave(colleagueId,nextSequenceId,userList){ &#xd;
	&#xd;
	var anexos = hAPI.listAttachments();&#xd;
	var temAnexos = false;&#xd;
	&#xd;
	for (i = 0; i &lt; anexos.size() ;i++){ &#xd;
		var anexoAtual = anexos.get(i);&#xd;
		if(anexoAtual.getDocumentDescription() == &quot;Proposta Comercial.pdf&quot;){&#xd;
			temAnexos = true;&#xd;
		}&#xd;
	}&#xd;
	&#xd;
&#xd;
	&#xd;
	if(!temAnexos){&#xd;
		throw(&quot;Proposta inicial não foi anexada.&quot;);&#xd;
	}&#xd;
	&#xd;
}</eventDescription>
    </WorkflowProcessEvent>
